{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/roadrunner-laravel"
        }
    ],
    "require": {
        "php": "^7.2",
        "beyondcode/laravel-websockets": "^1.3",
        "elasticsearch/elasticsearch": "^7.3",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hamburgscleanest/guzzle-advanced-throttle": "^4.1",
        "imtigger/laravel-job-status": "^0.2.2",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.1",
        "laravel/horizon": "^3.3",
        "laravel/passport": "^7.5",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^3.0",
        "mhor/php-mediainfo": "^4.1",
        "patrickschur/language-detection": "^3.4",
        "predis/predis": "^1.1",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-permission": "^3.0",
        "symfony/dom-crawler": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel-frontend-presets/tailwindcss": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "wnx/laravel-stats": "^2.0"
    },
    "config": {
        "platform": {
            "php": "7.3.10"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
